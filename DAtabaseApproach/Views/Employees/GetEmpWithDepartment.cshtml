@model List<DAtabaseApproach.Models.EmpJoinDept>
@{
    ViewBag.Title = "GetEmpWithDepartment";
}

<h2>GetEmpWithDepartment</h2>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
        <th>DeptName</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
    <td>@item.EmpName</td>
    <td>@item.EmpSalary</td>
    <td>@item.DeptName</td>
</tr>

    }
</table>
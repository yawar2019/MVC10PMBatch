@model List<MVC10PMBatch.Models.EmployeeModel>
@{
    ViewBag.Title = "getAllEmployee";
}

<h2>getAllEmployee</h2>

<table class="table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }

</table>